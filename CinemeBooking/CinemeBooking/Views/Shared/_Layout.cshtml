<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat&display=swap" rel="stylesheet" />
    @Styles.Render("~/assets/themify-icons/themify-icons.css")
    @Styles.Render("~/assets/fontawesome-free-6.4.0-web/css/all.min.css")
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    @Styles.Render("~/assets/Styles/Base.css")
    @Styles.Render("~/assets/Styles/Home.css")
    @Styles.Render("~/assets/Styles/Film-Details.css")
    @Styles.Render("~/assets/Styles/BookTickets.css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="app">
        @RenderBody()
    </div>
    <script>
        const query = document.querySelector.bind(document);
        const queryAll = document.querySelectorAll.bind(document);
    </script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
